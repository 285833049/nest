<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="com.jovezhao.nest.core.cache.CacheFactory">
        <property name="strategies">
            <set>
                <bean class="com.jovezhao.nest.core.cache.CacheGroupStrategy">
                    <property name="name" value="entityObject"/>
                    <property name="idleSeconds" value="10"/>
                    <property name="provider" ref="defaultCacheProvider"/>
                </bean>
            </set>
        </property>

    </bean>
    <bean name="defaultCacheProvider" class="DefaultCacheProvider"></bean>
    <bean name="Person_Repository" class="PersonRepository"/>
    <bean name="Teacher_Repository" class="TeacherRepository"/>
    <bean class="com.jovezhao.nest.ddd.AbstractUnitOfWork"/>
</beans>